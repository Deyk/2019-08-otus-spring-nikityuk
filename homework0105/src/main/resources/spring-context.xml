<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="quizSourceFile" class="ru.otus.spring.homework0105.domain.QuizSourceFile">
        <constructor-arg value="quiz.csv"/>
    </bean>

    <bean id="quizDao" class="ru.otus.spring.homework0105.dao.impl.QuizDaoImpl"/>

    <bean id="quizService" class="ru.otus.spring.homework0105.service.impl.QuizServiceImpl">
        <constructor-arg ref="quizDao"/>
    </bean>


</beans>
